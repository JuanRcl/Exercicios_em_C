algoritmo "x"
var
media, m1, m2, m3:real

inicio
escreva("Digite a nota 1: ")
leia (m1)
escreva("Digite a nota 2: ")
leia (m2)
escreva("Digite a nota 3: ")
leia (m3)
media := (m1+m2+m3)/3 ;
se(media>=5) entao
   escreva ("Parabéns, você foi aprovado")
senao
     escreva ("Reprovado, se esforçe mais ano que vem")
fimse
fimalgoritmo

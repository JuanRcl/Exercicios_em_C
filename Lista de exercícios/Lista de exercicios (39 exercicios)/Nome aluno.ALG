algoritmo "Nome aluno"
var
n1, n2, n3, media: real
nomea: caractere
inicio
escreva("Digite o nome do aluno: ")
  leia(nomea)
escreval ("Digite as três notas do aluno: ")
  leia(n1, n2, n3)
media <- (n1+n2+n3)/3
escreval()
  escreval("A seguir o nome do aluno e sua média")
escreval()
escreval("Aluno: ",nomea)
escreval("A média é: ",media)
fimalgoritmo